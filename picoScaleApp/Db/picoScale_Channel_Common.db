# PicoScale Channels' datasources

record(longin, "$(C):$(P):POS_$(Ch)"){
	field(DESC, "Position $(Ch)")
	field(DTYP, "asynInt32")
	field(SCAN, "I/O Intr")
	field(EGU, "nm")
	field(INP, "@asyn($(PORTNAME),$(ADDRESS),$(TIMEOUT))POS_CH$(Ch)_WAVEFORM_VAL")
}

